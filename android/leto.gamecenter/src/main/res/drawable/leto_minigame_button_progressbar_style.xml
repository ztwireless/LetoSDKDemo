<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:id="@android:id/background">
        <shape>
            <corners android:radius="@dimen/progressbar_radiu" />
            <gradient
                android:angle="270.0"
                android:centerColor="@color/mgc_sdk_greenss"
                android:centerY="0.75"
                android:endColor="@color/mgc_sdk_greenss"
                android:startColor="@color/mgc_sdk_greenss" />
        </shape>
    </item>


    <item android:id="@android:id/secondaryProgress">
        <clip>
            <shape>
                <corners android:radius="@dimen/progressbar_radiu" />
                <gradient
                    android:angle="90.0"
                    android:centerColor="#ac6079"
                    android:centerY="0.75"
                    android:endColor="#6c213a"
                    android:startColor="#e71a5e" />
            </shape>
        </clip>
    </item>

    <item android:id="@android:id/progress">
        <clip>
            <shape>
                <corners android:radius="@dimen/progressbar_radiu" />
                <gradient
                    android:angle="0"
                    android:endColor="@color/mgc_sdk_blue3"
                    android:startColor="@color/mgc_sdk_blue3" />
            </shape>
        </clip>
    </item>

</layer-list>
